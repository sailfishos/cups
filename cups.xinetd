# default: on
# description: The cups server is a print server. Woo, printing!
service ipp
{
	disable	= no
	flags		= IPv4
	port		= 631
	socket_type     = stream
	wait            = yes
	user            = root
	server          = /usr/sbin/cupsd
	server_args     = -x
	log_on_failure  += USERID
	bind		= 127.0.0.1
}

